@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "MD5加密,MD5解密";
    ViewBag.Keywords = "MD5解密,MD5解密";
    ViewBag.Description = "MD5解密,MD5解密";
}

<ul class="nav nav-tabs">
    <li class="active"><a href="/encrypt/md5">Md5</a></li>
    <li><a href="/encrypt/base64">Base64</a></li>
    <li><a href="/encrypt/image64">图片转Base64</a></li>
    <li><a href="/encrypt/filemd5">文件Md5</a></li>
</ul>
<div class="tab-content">
    <div class="tab-pane active">
        <table class="compare-table">
            <tr>
                <td class="compare-first">
                    <textarea spellcheck="false" id="txtSourceMd5" class="form-control " rows="18">666666</textarea>
                </td>
                <td class="compare-second">
                    <p>
                        <input id="rad16" type="radio" name="radHalf" /><label class="lblNormal" for="rad16">16位</label>
                    </p>
                    <p>
                        <input id="rad32" type="radio" name="radHalf" checked="checked" /><label class="lblNormal" for="rad32">32位</label>
                    </p>
                    <p><button id="btnMd5" class="btn btn-primary">MD5 &gt;</button></p>
                </td>
                <td class="compare-third">
                    <textarea spellcheck="false" id="txtDestMd5" class="form-control" rows="18"></textarea>
                </td>
            </tr>
        </table>
    </div>
    @Html.Partial("_ErrorTip")
</div>
@section Script{
    <script src="/assets/js/encrypt/md5.js"></script>
}