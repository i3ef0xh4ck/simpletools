@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "时间戳转换,Unix时间戳转日期";
    ViewBag.Keywords = "时间戳转换,Unix时间戳转日期";
    ViewBag.Description = "时间戳转换,Unix时间戳转日期";
}

<ul class="nav nav-tabs">
    <li><a href="/transfer/dbc">全角半角</a></li>
    <li><a href="/transfer/upper">大小写转换</a></li>
    <li class="active"><a href="/transfer/unix">Unix时间戳</a></li>
    <li><a href="/transfer/len">字符长度</a></li>
    <li><a href="/transfer/guid">GUID</a></li>
    <li><a href="/transfer/url">URL参数格式化</a></li>
</ul>
<div class="tab-content">
    <div class="tab-pane active">
        <table class="compare-table">
            <tr>
                <td class="compare-first">
                    <table class="inner-table">
                        <tr>
                            <td class="input-group">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button">原日期</button>
                                </span>
                                <input id="txtSourceUnix" type="text" class="form-control" value="@string.Format("{0:yyyy-MM-dd HH:mm:ss}",DateTime.Now)" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm:ss' })">
                                <span class="input-group-btn">
                                    <button id="btnTransUnix" data-val="10" class="btn btn-primary btnCal" type="button">转时间戳</button>
                                </span>
                            </td>
                        </tr>
                        <tr>
                            <td class="input-group">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button">时间戳</button>
                                </span>
                                <input id="txtDestUnix" type="text" class="form-control">
                                <span class="input-group-btn">
                                    <button id="btnTransDate" data-val="2" class="btn btn-primary btnCal" type="button">转原日期</button>
                                </span>
                            </td>
                        </tr>
                    </table>
                </td>
                <td class="compare-second"></td>
                <td class="compare-third"></td>
            </tr>
        </table>
    </div>
    @Html.Partial("_ErrorTip")
</div>
@section Script{
    <script src="/assets/plugins/DatePicker/WdatePicker.js"></script>
    <script src="/assets/js/transfer/unix.js"></script>
}
