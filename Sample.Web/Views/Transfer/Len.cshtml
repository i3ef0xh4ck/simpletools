</ul>
<div class="tab-content">
    <div class="tab-pane active">
        <textarea spellcheck="false" id="txtSourceLen" class="form-control " rows="18">hello world!世界</textarea>
        <p class="mt15">
            <div class="alert alert-warning alert-dismissible compress-tip" role="alert">
                压缩比：100%
            </div>
            <div class="alert alert-warning alert-dismissible length-tip" role="alert" style="display: none;"></div>
            <span id="btnGetLen" class="btn btn-primary">计算</span>
        </p>
    </div>
    @Html.Partial("_ErrorTip")
</div>
@section Script{
    <script src="/assets/js/transfer/len.js"></script>
}
