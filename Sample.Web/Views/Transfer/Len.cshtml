@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "字符串长度计算，文本长度计算";
    ViewBag.Keywords = "字符串长度计算，文本长度计算";
    ViewBag.Description = "字符串长度计算，文本长度计算";
}

<ul class="nav nav-tabs">
    <li><a href="/transfer/dbc">全角半角</a></li>
    <li><a href="/transfer/upper">大小写转换</a></li>
    <li><a href="/transfer/unix">Unix时间戳</a></li>
