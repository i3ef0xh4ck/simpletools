@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "UrlEncode编码,UrlEncode解码";
    ViewBag.Keywords = "UrlEncode编码,UrlEncode解码";
    ViewBag.Description = "UrlEncode编码,UrlEncode解码";
}

<ul class="nav nav-tabs">
    <li class="active"><a href="/encode/url">UrlEncode</a></li>
    <li><a href="/encode/html">HtmlEncode</a></li>
    <li><a href="/encode/ascii">ASCII</a></li>
    <li><a href="/encode/utf8">UTF-8</a></li>
    <li><a href="/encode/unicode">Unicode</a></li>
</ul>
<div class="tab-content">
    <div class="tab-pane active">
        <table class="compare-table">
            <tr>
                <td class="compare-first">
                    <textarea id="txtSourceUrl" class="form-control " rows="18">http://www.baidu.com?t=hello</textarea>
                </td>
                <td class="compare-second">
                    <p><button id="btnUrlEncode" class="btn btn-primary">编码 &gt;</button></p>
                    <p><button id="btnUrlDecode" class="btn btn-primary">&lt; 解码</button></p>
                </td>
                <td class="compare-third">
                    <textarea id="txtDestUrl" class="form-control" rows="18"></textarea>
                </td>
            </tr>
        </table>
    </div>
    @Html.Partial("_ErrorTip")
</div>
@section Script{
    <script src="/assets/js/encode/urlencode.js"></script>
}






